{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "Nuxt: Firefox",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Nuxt: Node",
      "outputCapture": "std",
      "program": "./node_modules/nuxt/bin/nuxt.js",
      "args": ["--port", "3000"]
    }
  ],
  "compounds": [
    {
      "name": "Nuxt: Full",
      "configurations": ["Nuxt: Node", "Nuxt: Firefox"]
    }
  ]
}
